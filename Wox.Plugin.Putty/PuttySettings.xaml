<UserControl x:Class="Wox.Plugin.Putty.PuttySettings"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Wox.Plugin.Putty"
             mc:Ignorable="d"
             Loaded="UserControl_Loaded"
             d:DesignHeight="300"
             d:DesignWidth="300">
    <Border BorderBrush="Gray" Margin="10" BorderThickness="1">
        <StackPanel>
            <CheckBox x:Name="AddPuttyExeInResults" Content="Add putty exe in results" Margin="10" HorizontalAlignment="Left" />
        </StackPanel>
    </Border>
</UserControl>